<?xml version="1.0" encoding="utf-8"?>
<merge
  xmlns:latin="http://schemas.android.com/apk/res/inputmethod.latin.ported">
    <include latin:keyboardLayout="@xml/kbd_key_styles" />
    <include latin:keyboardLayout="@xml/kbd_currency_key_styles" />
    <Row latin:keyWidth="10.000002%p">
        <Key latin:keyLabel="~" />
        <Key latin:keyLabel="`" />
        <Key latin:keyLabel="|" />
        <Key latin:moreKeys="@string/more_keys_for_bullet" latin:keyLabel="•" />
        <Key latin:keyLabel="√" />
        <Key latin:moreKeys="Π" latin:keyLabel="π" />
        <Key latin:keyLabel="÷" />
        <Key latin:keyLabel="×" />
        <Key latin:keyLabel="{" />
        <Key latin:keyLabel="}" latin:keyWidth="fillRight" />
    </Row>
    <Row latin:keyWidth="10.000002%p">
        <Key latin:keyStyle="nonSpecialBackgroundTabKeyStyle" />
        <Key latin:keyStyle="moreCurrency1KeyStyle" />
        <Key latin:keyStyle="moreCurrency2KeyStyle" />
        <Key latin:keyStyle="moreCurrency3KeyStyle" />
        <Key latin:moreKeys="′,″" latin:keyLabel="°" />
        <Key latin:moreKeys="↑,↓,←,→" latin:keyLabel="^" />
        <Key latin:keyLabel="_" />
        <Key latin:moreKeys="≠,≈,∞" latin:keyLabel="=" />
        <Key latin:keyLabel="[" />
        <Key latin:keyLabel="]" latin:keyWidth="fillRight" />
    </Row>
    <Row latin:keyWidth="10.000002%p">
        <Key latin:keyStyle="backFromMoreSymbolKeyStyle" latin:visualInsetsRight="0.99999905%p" latin:keyWidth="14.999998%p" />
        <Key latin:keyLabel="™" />
        <Key latin:keyLabel="®" />
        <Key latin:keyLabel="©" />
        <Key latin:moreKeys="§" latin:keyLabel="¶" />
        <Key latin:keyLabel="\\" />
        <Key latin:moreKeys="≤,«,‹" latin:keyLabel="&lt;" />
        <Key latin:moreKeys="≥,»,›" latin:keyLabel=">" />
        <Key latin:keyStyle="deleteKeyStyle" latin:visualInsetsLeft="0.99999905%p" latin:keyWidth="fillRight" />
    </Row>
    <include latin:keyboardLayout="@xml/kbd_symbols_shift_row4" />
</merge>
